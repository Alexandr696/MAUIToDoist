@page "/"

<h1>Hello, world!</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Название</th>
            <th>Статус</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in List)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.IsDone</td>
            </tr>
        }
    </tbody>
</table>


<button @onclick="Click">cccc</button>

